<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/eventListener.xsd">
    <import>
        <eventlistener name="AbsenceSettings">
            <eventclassname>wcf\form\AbsenceSettingsForm</eventclassname>
            <eventname>assignVariables,readFormParameters,readParameters,save</eventname>
            <environment>user</environment>
            <inherit>1</inherit>
            <listenerclassname>wcf\system\event\listener\AbsenceSettingsFormListener</listenerclassname>
        </eventlistener>

        <eventlistener name="AbsenceConversationAction">
            <eventclassname>wcf\data\conversation\ConversationAction</eventclassname>
            <eventname>finalizeAction</eventname>
            <environment>user</environment>
            <listenerclassname>wcf\system\event\listener\AbsenceConversationListener</listenerclassname>
        </eventlistener>
    </import>
</data>
